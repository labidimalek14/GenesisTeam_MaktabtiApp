<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" stylesheets="@styles/fxmlmaktabti.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.esprit.gui.FXMLAjouterReclamationController">
   <children>
      <TextField fx:id="Feedback" layoutX="213.0" layoutY="91.0" prefHeight="31.0" prefWidth="281.0" promptText="votre feedback" />
